{
    "name": "jose-soluct/php-nfse",
    "type": "library",
    "description": "Pacote para emiss√£o de NFSe",
    "keywords": ["nfse", "sped", "nfephp"],
    "license": ["Apache-2.0"],
    "require": {
        "php" : "~8",
        "nfephp-org/sped-common" : "^v5",
        "respect/validation": "^2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^11",
        "scrutinizer/ocular": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "NFePHP\\NFSe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NFePHP\\NFSe\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.*-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
